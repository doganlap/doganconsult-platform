{
  "App": {
    "SelfUrl": "https://dc.doganconsult.com",
    "RedirectAllowedUrls": "https://dc.doganconsult.com"
  },
  "AuthServer": {
    "Authority": "https://dc.doganconsult.com",
    "RequireHttpsMetadata": true
  },
  "RemoteServices": {
    "Default": {
      "BaseUrl": "http://localhost:5000"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5001"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
