{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      },
      "Https": {
        "Url": "https://0.0.0.0:5001"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "identity-route": {
        "ClusterId": "identity-cluster",
        "Match": {
          "Path": "/api/identity/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "organization-route": {
        "ClusterId": "organization-cluster",
        "Match": {
          "Path": "/api/organization/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "workspace-route": {
        "ClusterId": "workspace-cluster",
        "Match": {
          "Path": "/api/workspace/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "userprofile-route": {
        "ClusterId": "userprofile-cluster",
        "Match": {
          "Path": "/api/userprofile/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/userprofile/{**catch-all}"
          }
        ]
      },
      "audit-route": {
        "ClusterId": "audit-cluster",
        "Match": {
          "Path": "/api/audit/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/audit/{**catch-all}"
          }
        ]
      },
      "document-route": {
        "ClusterId": "document-cluster",
        "Match": {
          "Path": "/api/document/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/document/{**catch-all}"
          }
        ]
      },
      "ai-route": {
        "ClusterId": "ai-cluster",
        "Match": {
          "Path": "/api/ai/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/ai/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "identity-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "organization-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "workspace-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5004"
          }
        }
      },
      "userprofile-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5005"
          }
        }
      },
      "audit-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5006"
          }
        }
      },
      "document-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5007"
          }
        }
      },
      "ai-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5008"
          }
        }
      }
    }
  }
}
